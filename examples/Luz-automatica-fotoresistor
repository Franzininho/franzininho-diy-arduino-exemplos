// Este codigo junto com as ligações eletronicas configura um fotoresistor a acender um led na ausencia de luz

int sensorValue = 0;

void setup()
{
  pinMode(A0, INPUT);
  Serial.begin(9600);
  pinMode(9, OUTPUT);
}

void loop()
{
  sensorValue = analogRead(A0);
  Serial.println(sensorValue);
  
  if(sensorValue>40){
  	 digitalWrite(9, LOW);
  }
  
  else{
     digitalWrite(9, HIGH);
  }
}
